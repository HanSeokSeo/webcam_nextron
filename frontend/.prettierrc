{
  "plugins": ["prettier-plugin-tailwindcss"],
  "arrowParens": "avoid",
  "templateLiteral": "preserve",
  "jsxBracketSameLine": true,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "bracketSameLine": true,
  "jsxSingleQuote": false,
  "printWidth": 120,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "singleQuote": false,
  "semi": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false
}
